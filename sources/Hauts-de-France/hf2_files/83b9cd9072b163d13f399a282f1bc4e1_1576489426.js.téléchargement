var config = {
sensitivity: 3, // number = sensitivity threshold (must be 1 or higher)
interval: 50,  // number = milliseconds for onMouseOver polling interval
over: doOpen,   // function = onMouseOver callback (REQUIRED)
timeout: 200,   // number = milliseconds delay before onMouseOut
out: doClose    // function = onMouseOut callback (REQUIRED)
};
function doOpen() {
$(this).addClass("hover");
$('div:first',this).css('visibility', 'visible');
$('ul:first',this).css('visibility', 'visible');
$('ul:first',this).css('z-index', '5000');
$("div.leaflet-top").css("z-index",'0')
}
function doClose() {
$(this).removeClass("hover");
$('div:first',this).css('visibility', 'hidden');
$('ul:first',this).css('visibility', 'hidden');
}
$("ul.dropdown li").hoverIntent(config);
$("ul.dropdown li a").focus(function () {
$(this).parent().find('ul').css('visibility', 'visible');
$(this).parent().find('div').css('visibility', 'visible');
$(this).parent().addClass("hover");
$(this).parent().find('ul').addClass("hover");
});
$("li.toggleSubMenu a").focus(function () {
$("ul.dropdown ul").css('visibility', 'hidden');
$("ul.dropdown div").css('visibility', 'hidden');
});
$("ul.dropdown li ul li:has(ul)").find("a:first").append(" &raquo; ");
$("#accordion").accordion();
if ($('#btReseauxSociaux').size() > 0) {
$('a.triggerAfficheReseauxSociaux').bind("click", afficheReseauxSociaux);
$('#btReseauxSociaux a.triggerFermerReseauxSociaux').bind("click", masqueReseauxSociaux);
}
});
$(".plus").click(function(){
if ($(this).parent().find('img.img_plus').attr('src')=='/extension/ide/design/ide_design/images/plus.png')
{
$(this).parent().find('img.img_plus').attr('src','/extension/ide/design/ide_design/images/moins.png');
$(this).parent().find('.plus_liste').removeClass('hidden');
}
else
{
$(this).parent().find('img.img_plus').attr('src','/extension/ide/design/ide_design/images/plus.png');
$(this).parent().find('.plus_liste').addClass('hidden');
}
});
function make_actus ()
{
reset_actus();
$('#main_actu_1').toggleClass('hidden');
$("#main_actu_1").animate({
opacity: 1
}, 500 );
}
function reset_actus ()
{
$('#main_actu_1').addClass('hidden');
$('#main_actu_2').addClass('hidden');
$('#main_actu_3').addClass('hidden');
$('#link_to_2').removeClass('on');
$('#link_to_3').removeClass('on');
}
$("#link_to_1 a").click(function(){
show_actu("#main_actu_1");
$(this).parent().addClass('on');
});
$("#link_to_2 a").click(function(){
show_actu("#main_actu_2");
$(this).parent().addClass('on');
});
$("#link_to_3 a").click(function(){
show_actu("#main_actu_3");
$(this).parent().addClass('on');
});
function show_actu($elemt)
{
reset_actus();
$($elemt).toggleClass('hidden');
$($elemt).animate({
opacity: 1
}, 500 );
}
$(".roll").mouseover(function(){
$(this).addClass('on');
});
$(".roll").mouseout(function(){
$(this).removeClass('on');
});
function afficheReseauxSociaux (evt)
{
evt.preventDefault();
$(this).parent().toggleClass('ouvert');
}
function masqueReseauxSociaux (evt)
{
evt.preventDefault();
$(this).parent().parent().parent().removeClass('ouvert');
}
